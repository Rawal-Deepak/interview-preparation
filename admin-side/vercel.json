{
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "admin-side/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/backend/server.js" },
      { "src": "/(.*)", "dest": "/admin-side/build/$1" }
    ]
  }
  